{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "@features/*": ["features/*"],
      "@components/*": ["components/*"],
      "@model/*": ["model/*"],
      "@util/*": ["util/*"]
    }
  }
}
